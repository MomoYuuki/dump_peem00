    <!--appTypeConfig unfreeze special app; key: cnpType(all, net, alarm, sync), value: appType-->
    <appTypeConfig cpnKey="alarm" appType="3"/>
    <appTypeConfig cpnKey="net" appType="4"/>
    <appTypeConfig cpnKey="sync" appType="8"/>

    <appType key="lcdoffRestoreNet" appType="6#11"/>

    <!-- alarm white (pkgName+action) -->

    <!-- alarm key pkg white (key pkgName) -->
    <alarmKeyPkg keyPkg="clock"/>
    <alarmKeyPkg keyPkg="alarm"/>
    <alarmKeyPkg keyPkg="calendar"/>
    <alarmKeyPkg keyPkg="com.xiasgcfytomgage.MysAlasrm" />

    <!-- fast freezer access list (pkgname) -->

    <!-- alarm key action white (key action) -->

    <!-- activity/service/provider component called white&black (pkgName+cpnName) -->
    <providerCpnCalled pkg="com.oppo.optvassistant" type="white" cpnName="com.oneplus.optvassistant.provider.DeviceAppProvider"/>
    <broadcastCpnCalled pkg="all" type="white" action="oppo.intent.action.opluscarlink.INIT_OPLUS_SDK"/>


    <!-- broadcast component called white&black (action+pkgName) -->


    <!-- component caller white&black (pkgName) -->
    <serviceCpnCaller pkg="com.heytap.mcs" type="white"/>
    <serviceCpnCaller pkg="com.android.systemui" type="white"/>

    <!-- lcdoff/lcdon prevent cpn awake (pkg, mask) maks={close_socket, aysnbinder, alarm, sync, job, broadcast, provider, bindservice, startservice, activity} -->
    <lcdoffPrevent pkg="com.google.android.gms" mask="0111111110"/>
    <lcdoffPrevent pkg="com.android.vending" mask="0111111110"/>
    <lcdoffPrevent pkg="com.google.android.partnersetup" mask="0111111110"/>
    <lcdoffPrevent-version pkg="com.xunmeng.pinduoduo" mask="1111111110" version="PEDM00#PEEM00#LE2110#LE2120#LE2100#PEGM10#PEPM00#PENM00#PEQM00#PEXM00"/>
    <lcdonPrevent-version pkg="com.xunmeng.pinduoduo" mask="1111111110" version="PEDM00#PEEM00#LE2110#LE2120#LE2100#PEGM10#PEPM00#PENM00#PEQM00#PEXM00"/>
    <!---airplan white cpn awake (pkg, mask) -->

    <!-- broadcast action black list action={broadcast action} pkg={pkg, all} mask={fastfreeze, lcdon, lcdoff} -->
    <brdBlack action="android.net.wifi.SCAN_RESULTS" pkg="all" mask="011" />
    <brdBlack action="android.intent.action.BATTERY_CHANGED" pkg="all" mask="011" />
    <brdBlack action="android.intent.action.CLOSE_SYSTEM_DIALOGS" pkg="all" mask="011" />
    <brdBlack action="android.intent.action.PACKAGE_RESTARTED" pkg="all" mask="011" />
    <brdBlack action="android.intent.action.SCREEN_ON" pkg="all" mask="011" />
    <brdBlack action="android.intent.action.CONFIGURATION_CHANGED" pkg="all" mask="011" />
    <brdBlack action="android.media.VOLUME_CHANGED_ACTION" pkg="all" mask="011" />
    <brdBlack action="android.intent.action.PHONE_STATE" pkg="all" mask="011" />
    <brdBlack action="android.intent.action.USER_PRESENT" pkg="all" mask="011" />
    <brdBlack action="com.bytedance.ttnet.config.appconfig.SYNC_MAIN_PROCESS_CONFIG" pkg="all" mask="011" />


    <!-- type 1: skip fastfreeze, 2: bumpservice white 3: bumpservice(unbind)black
            4:LcdoffPreventRestoreNetWhiteList 5:LcdoffGameCloseNetWhiteList 6:LcdonDisableJobBlackList
			11: kill app-->
    <configPkg type="1" pkg="com.android.permissioncontroller"/>
    <configPkg type="1" pkg="com.google.android.permissioncontroller"/>
    <configPkg type="1" pkg="android"/>
    <configPkg type="1" pkg="com.android.settings"/>
    <configPkg type="1" pkg="com.google.android.packageinstaller"/>
    <configPkg type="3" pkg="com.tencent.qqlive"/>
    <configPkg type="7" pkg="com.hiby.music"/>
    <configPkg type="7" pkg="com.kugou.viper"/>
    <configPkg type="11" pkg="com.tencent.qqlive"/>
    <configPkg type="11" pkg="com.ss.android.article.lite"/>

    <providerCpnCaller pkg="com.coloros.oshare" type="white"/>

    <!-- async binder white (aidlName+pkgName/code) -->
    <asyncBinder desc="android.media.IAudioFocusDispatcher" type="pkg" pkg="all"/>
    <asyncBinder desc="android.media.IAudioPolicyServiceClient" type="pkg" pkg="all"/>
    <asyncBinder desc="android.media.IAudioFlingerClient" type="pkg" pkg="all"/>
    <asyncBinder desc="android.service.controls.IControlsProvider" pkg="all" type="pkg" />
    <asyncBinder desc="android.bluetooth.IBluetoothGattCallback" type="code" method="TRANSACTION_onClientConnectionState"/>
    <asyncBinder desc="android.bluetooth.IBluetoothGattCallback" type="code" method="TRANSACTION_onConnectionUpdated"/>
    <asyncBinder desc="android.os.IMessenger" type="pkg" pkg="com.heytap.opluscarlink"/>
    <asyncBinder desc="android.app.IApplicationThread" type="code" method="TRANSACTION_dumpService"/>
    <asyncBinder desc="android.app.IApplicationThread" type="code" method="TRANSACTION_dumpHeap"/>
    <asyncBinder desc="android.app.IApplicationThread" type="code" method="TRANSACTION_dumpActivity"/>
    <asyncBinder desc="android.app.IApplicationThread" type="code" method="TRANSACTION_dumpMemInfo"/>
    <asyncBinder desc="android.app.IApplicationThread" type="code" method="TRANSACTION_dumpMemInfoProto"/>
    <!-- async binder black (aidlName+pkgName/code) -->
    <asyncBinderBlack desc="com.android.internal.telephony.IPhoneStateListener" type="pkg" pkg="com.tencent.mm"/>
    <asyncBinderBlack desc="com.android.internal.telephony.IPhoneStateListener" type="pkg" pkg="com.tencent.mobileqq"/>

    <asyncBinderBlack desc="android.media.IAudioPolicyServiceClient" type="pkg" pkg="com.xunmeng.pinduoduo#com.ss.android.article.lite#com.ss.android.article.video#com.ss.android.ugc.aweme"/>
    <asyncBinderBlack desc="android.media.IAudioPolicyServiceClient" type="pkg" pkg="com.smile.gifmaker#com.baidu.searchbox#com.autonavi.minimap#com.taobao.taobao#com.UCMobile#com.jifen.qukan"/>
    <asyncBinderBlack desc="android.media.IAudioFlingerClient" type="pkg" pkg="com.xunmeng.pinduoduo#com.ss.android.article.lite#com.ss.android.article.video#com.ss.android.ugc.aweme"/>
    <asyncBinderBlack desc="android.media.IAudioFlingerClient" type="pkg" pkg="com.smile.gifmaker#com.baidu.searchbox#com.autonavi.minimap#com.taobao.taobao#com.UCMobile#com.jifen.qukan"/>

    <asyncBinderCallerWhite desc="android.os.IMessenger" pkg="com.heytap.opluscarlink"/>

    <!-- broadcast pending (action) -->
    <broadcastPending action="android.intent.action.TIME_TICK"/>
    <broadcastPending action="android.intent.action.BATTERY_CHANGED"/>
    <broadcastPending action="android.intent.action.ANY_DATA_STATE"/>

